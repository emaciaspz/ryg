<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="format-detection" content="telephone=no">
    <meta name="msapplication-tap-highlight" content="no">
    <meta name="viewport" content="initial-scale=1, maximum-scale=1, user-scalable=no, width=device-width">
    <!--Onsen UI-->
    <link rel="stylesheet" type="text/css" href="css/onsenui.css" />
    <link rel="stylesheet" type="text/css" href="css/onsen-css-components.min.css" />
    <script type="text/javascript" src="js/onsenui.min.js"></script>
    <!--Jquery-->
    <link rel="stylesheet" type="text/css" href="css/jquery.mobile-1.4.5.css" />
    <link rel="stylesheet" type="text/css" href="css/jquery.toastmessage.css" />
    <!--Bootstrap-->
    <link rel="stylesheet" type="text/css" href="css/bootstrap.min.css" />
    <!--ToastR-->
    <link rel="stylesheet" type="text/css" href="css/toastr.min.css" />
    <!--Estilos Personalizados-->
    <link rel="stylesheet" type="text/css" href="css/style.css" />

    <!--Jquery-->
    <script type="text/javascript" src="js/jquery.min.js"></script>
    <script type="text/javascript" src="js/jquery.validate.js"></script>
    <!--Jquery Mobile-->
    <script type="text/javascript" src="js/jquery.mobile-1.4.5.min.js"></script>
    <!-- Toast-->
    <script type="text/javascript" src="js/vanillatoasts.js"></script>
    <link rel="stylesheet" type="text/css" href="css/vanillatoasts.css"></script>
    <!--Moment Js-->
    <script type="text/javascript" src="js/moment.js"></script>
    <!-- Numeral Js -->
    <script type="text/javascript" src="js/numeral.min.js"></script>
    <!--Funciones-->
    <script type="text/javascript" src="js/applog.js"></script>
    <script type="text/javascript" src="js/login/login.js"></script>
    <script type="text/javascript" src="js/login/DialogoCerrarSesion.js"></script>
    <script type="text/javascript" src="js/login/DialogoNuevoUsuario.js"></script>
    <script type="text/javascript" src="js/login/DialogoRestablecer.js"></script>
    <script type="text/javascript" src="js/buscar.js"></script>
    <script type="text/javascript" src="js/datos.js"></script>
    <script type="text/javascript" src="js/datosUsuario.js"></script>
    <script type="text/javascript" src="js/bitacora/bitacora.js"></script>
    <script type="text/javascript" src="js/gasto/GastosService.js"></script>
    <script type="text/javascript" src="js/gasto/TablaDetalleGasto.js"></script>
    <script type="text/javascript" src="js/gasto/gastos.js"></script>
    <script type="text/javascript" src="js/inspeccion/inspeccion.js"></script>
    <script type="text/javascript" src="js/inspeccion/InspeccionFotografia.js"></script>
    <script type="text/javascript" src="js/inspeccion/InspeccionService.js"></script>
    <script type="text/javascript" src="js/inspeccion/InspeccionSubirDocumento.js"></script>
    <script type="text/javascript" src="js/inspeccion/InspeccionSubirPDF.js"></script>
    <script type="text/javascript" src="js/inspeccion/InspeccionTablaFotografias.js"></script>
    <script type="text/javascript" src="js/catalogos.js"></script>    
    <script type="text/javascript" src="js/peticiones/PeticionesService.js"></script>
    <script type="text/javascript" src="js/peticiones/peticiones.js"></script>
    <script type="text/javascript" src="js/peticiones/DescripcionPeticion.js"></script>
    <script type="text/javascript" src="js/peticiones/FormatearPeticion.js"></script>
    <script type="text/javascript" src="js/peticiones/ListadoPeticiones.js"></script>
    <script type="text/javascript" src="js/ref.js"></script>
    <script type="text/javascript" src="js/documento.js"></script>
    <script type="text/javascript" src="js/pdf.js"></script>
    <script type="text/javascript" src="js/pdfTemplate.js"></script>
    <script type="text/javascript" src="js/fotografias.js"></script>
    <script type="text/javascript" src="js/bitacora/BitacoraService.js"></script>
    <!--Utilerías-->
    <script type="text/javascript" src="js/baseDatos.js"></script>
    <script type="text/javascript" src="js/uploadFile.js"></script>
    <script type="text/javascript" src="js/utils.js"></script>
    <script type="text/javascript" src="js/"></script>
    <script type="text/javascript" src="js/main.js"></script>
    <title>Kronos Móvil</title>
</head>

<body>
    <!--User Panel-->
    <ons-splitter id="panelUsuario">
        <ons-splitter-side id="menu" side="right" width="220px" collapse modifier="longdivider">
            <ons-page id="pageUserPane">
                <div id="paneContent"></div>
            </ons-page>
        </ons-splitter-side>
        <ons-splitter-content id="content" page="index.html"></ons-splitter-content>
    </ons-splitter><!--/User panel-->

    <!--Pantalla Index-->
    <ons-template id="index.html">
        <ons-page id="index-page">
        </ons-page>
    </ons-template><!--Pantalla index-->

    <!--Login-->
    <ons-template id="login.html">
        <ons-page id="login-page">
            <div style="text-align: center; margin-top: 30px;">
                <ons-toolbar style="background-color: #008BFB;">
                        <a href="http://rygespinosa.com/" class="left" target="_blank">
                            <ons-toolbar-button class="button--quiet left btnLoginTb">
                                <label style="color: #fff;font-size: 14px;">PÁGINA WEB</label>
                            </ons-toolbar-button>
                        </a>
                        <a href="mailto:contacto@rygespinosa.com" class="right">
                            <ons-toolbar-button class="button--quiet right">
                                <label style="color: #fff;font-size: 14px;">CONTACTO</label>        
                            </ons-toolbar-button>
                        </a>
                    
                </ons-toolbar>
                <br />
                <br />
                <img src="img/Logo_RG_V1.png" width="215px" class="shadow-sm rounded">
            </div>
            <br>
            <br>
            <br>
            <div style="text-align: center; margin-top: 30px;">
                <ons-card class="center card-op" style="background:#002955;width:325px;height:310px;">
                    <div class="center icon-login">
                        <img class="center" src="img/users.png" width="125px">
                    </div>
                    <div id="divRut" class="input-group shadow-sm center" style="width: 260px;">
                        <div id="divRut2" class="input-group-prepend">
                            <span id="divRut3" class="input-group-text border-0 text-white" style="background-color: #008BFB;">
                                <ons-icon icon="md-account-circle"></ons-icon>
                            </span>
                        </div>
                        <input style="width: 220px;" id="rut" name="rut" 
                            placeholder="Nombre De Usuario" class="form-control border-0 focus-none"/>
                    </div>
                    <div id="divClave" class="input-group shadow-sm center" style="width: 260px;">
                        <div class="input-group-prepend">
                            <span class="input-group-text border-0 lock text-white" style="background-color: #008BFB;">
                                <ons-icon icon="lock"></ons-icon>
                            </span>
                        </div>
                        <input style="width: 220px;" id="clave" name="clave" 
                            type="password" placeholder="Clave" float class="form-control border-0 focus-none"/>
                    </div>
                        <button class="button center" onclick="revisarLogin();" id="btnLogin" style="vertical-align:middle;background-color: #008BFB;">
                                Ingresar
                        </button>
                    <br>
                </ons-card>
            </div>
            <!-- Footer -->
            <ons-bottom-toolbar>
                <center>
                    <strong>Versi&oacute;n 1.27</strong>
                </center>   
            </ons-bottom-toolbar>
        </ons-page>
    </ons-template><!-- Login -->

    <!--Buscar siniestro-->
    <ons-template id="buscar.html">
        <ons-page id="buscar">
            <!-- Header -->
            <ons-toolbar>
                <div class="right">
                    <ons-toolbar-button onclick="fn.open()">
                        <ons-icon icon="ion-navicon, material:md-menu" class="menu"></ons-icon>
                    </ons-toolbar-button>
                </div>
                <div class="center rounded">
                    <img src="img/image001.png" width="100px" class="rounded" vspace="15" hspace="15" onclick="cambiarSiniestro();">
                </div>
            </ons-toolbar><!-- /Header -->

            <ons-card style="background-color: #0081BD;">
                <div class="tituloKronos">
                    <h3>
                        <center>
                            Búsqueda de Siniestro
                        </center>
                    </h3>
                </div>
            </ons-card>

            <!-- Content -->
            <ons-card class="diamond" style="height: 495px;">
                <center>
                    <br/>
                    <input type="search" value="" id="buscarInput" placeholder="Buscar" class="search-input center"
                        style="width: 300px; height: 50px;background-color: #fff;">
                    <br/><br/>
                    <div>
                        <ons-card style="box-shadow: none;border: none;background-color: #fff;width: 300px;">
                            <ons-button modifier="quiet" style="font-size: 18px;width: 150px;"
                                onclick="initResultadosSiniestro(1);" class="center">
                                <ons-icon icon="md-search" style="width: 25px; color: #FE8416;"></ons-icon>
                                Buscar
                            </ons-button>
                            <ons-button modifier="quiet" onclick="initRefSin();"
                                style="font-size: 18px;width: auto;" class="center">
                                <ons-icon icon="md-sign-in" style="color: #FE8416;"></ons-icon>
                                Ir a Ref. R&G
                            </ons-button>
                        </ons-card>
                        <br/><br/>
                    </div>
                </center>
            </ons-card><!-- /Content -->
        </ons-page>
    </ons-template>
    <!--/Buscar Siniestro-->

    <!--Referencia R&G-->
    <ons-template id="ref.html">
        <ons-page id="ref">
            <ons-toolbar>
                <div class="right">
                    <ons-toolbar-button onclick="fn.open()">
                        <ons-icon icon="ion-navicon, material:md-menu" class="menu"></ons-icon>
                    </ons-toolbar-button>
                </div>
                <div class="center" id="titleApp"></div>
            </ons-toolbar>

            <div style="min-height: 120px; padding: 1px 0 0 0;">
                <div class="card card--material" style="background-color: #0081BD; border: 15px;">
                    <center>
                        <h2 class="card__title" style="color: #FFFFFF;" id="numeroSiniestro"></h2>
                        <p class="card-text" style="color: #FFFFFF" id="estatusSiniestro"></p>
                    </center>
                </div>
            </div>
            <ons-card>
                <center>
                    <ons-button modifier="quiet" id="btnDatos" onclick="datos();"
                        style="font-size: 22px;width: 280px;color: #0081BD;" class="center">
                        <ons-icon icon="md-folder-outline" style="color: #FE8416;"></ons-icon>
                        Datos
                    </ons-button>
                    <br>
                    <ons-button modifier="quiet" id="btnGastos" onclick="consultarGastos(1);"
                        style="font-size: 22px;width: 280px;color: #0081BD;" class="center">
                        <ons-icon icon="md-money" style="color: #FE8416;"></ons-icon>
                        Gastos
                    </ons-button>
                    <br>
                    <ons-button modifier="quiet" id="btnBitacora" onclick="bitacora();"
                        style="font-size: 22px;width: 280px;color: #0081BD;" class="center">
                        <ons-icon icon="md-time-restore" style="color: #FE8416;"></ons-icon>
                        Bitácora
                    </ons-button>
                    <br>
                    <ons-button modifier="quiet" id="btnInspeccion" onclick="inspeccion(1);"
                        style="font-size: 22px;width: 280px;color: #0081BD;" class="center">
                        <ons-icon icon="md-calendar" style="color: #FE8416;"></ons-icon>
                        Inspección
                    </ons-button>
                </center>
            </ons-card>
            <ons-card class="diamond-sm" style="height: 200px;"></ons-card>
        </ons-page>
    </ons-template>

    <!--Datos del siniestro-->
    <ons-template id="datos.html">
        <ons-page id="datos">
            <!-- Header -->
            <ons-toolbar>
                <div class="right">
                    <ons-toolbar-button onclick="fn.open()">
                        <ons-icon icon="ion-navicon, material:md-menu" class="menu"></ons-icon>
                    </ons-toolbar-button>
                </div>
                <div class="center" id="titleApp">
                    
                </div>
            </ons-toolbar><!-- /Header -->

            <ons-card style="background-color: #0081BD;">
                <div class="tituloKronos">
                    <h2 class="center">
                        <ons-icon icon="md-folder-outline"></ons-icon>
                        Datos
                    </h2>
                </div>
            </ons-card>
            <ons-card>
                <div id="datosSiniestro"></div>
            </ons-card>
            <ons-card class="diamond-sm" style="height:140px;"></ons-card>
        </ons-page>
    </ons-template>
    <!--Datos del siniestro-->

    <!--Resultados de Siniestro-->
    <ons-template id="resultadosSiniestro.html">
        <ons-page id="resultadosSiniestro">
            <!--Header-->
            <ons-toolbar>
                <div class="right">
                    <ons-toolbar-button onclick="fn.open();">
                        <ons-icon icon="ion-navicon, material:md-menu" class="menu"></ons-icon>
                    </ons-toolbar-button>
                </div>
                <div class="center rounded">
                    <img src="img/image001.png" width="100px" class="rounded" vspace="15" hspace="15" onclick="cambiarSiniestro();">
                </div>
            </ons-toolbar>
            <!--/header-->

            <center>
                <ons-card style="background-color: #0081BD;">
                    <div class="tituloKronos">
                        <h3 class="center">
                            <center>
                                Resultados de Búsqueda
                            </center>
                        </h3>
                    </div>
                </ons-card>

                <ons-card>
                    <div id="siniestrosContent"></div>
                </ons-card>
            </center>
        </ons-page>
    </ons-template><!--Resultados del siniestro-->

    <!--Gastos-->
    <ons-template id="gastos.html">
        <ons-page id="gastos">
            <ons-toolbar>
                <div class="right">
                    <ons-toolbar-button onclick="fn.open()">
                        <ons-icon icon="ion-navicon, material: md-menu"></ons-icon>
                    </ons-toolbar-button>
                </div>
                <div class="center" id="titleApp"></div>
            </ons-toolbar>
            <ons-card style="background-color: #0081BD;">
                <div class="tituloKronos">
                    <h2 class="center">
                        <ons-icon icon="md-money"></ons-icon>
                        &nbsp;Gastos
                    </h2>
                </div>
            </ons-card>
            <ons-card style="padding: 0;">
                <div class="content">  
                    <div style="padding-top: 1rem; padding-bottom: 1rem; text-align: center;">
                        <ons-button id="btnNuevoReporteGasto" disabled modifier="quiet" class="center" onclick="initNuevoGasto();"
                            style="color: #0081BD;font-size: 1rem;">
                                <ons-icon icon="plus-square" style="color: #FE8416;"></ons-icon>
                                Nuevo Reporte 
                        </ons-button>
                        <ons-button id="btnNuevoReporteGasto" modifier="quiet" class="center" onclick="actualizarListadoGasto();"
                            style="color: #0081BD;font-size: 1rem;">
                            <ons-icon icon="refresh" style="color: #FE8416;"></ons-icon>
                            Actualizar
                        </ons-button>
                    </div>
                    <div id="gastosContent">
                    </div>
                </div>
            </ons-card>
        </ons-page>
    </ons-template><!--Gastos-->

    <!-- Nuevo Reporte Gasto -->
    <ons-template id="nuevoReporteGasto.html">
        <ons-page id="nuevoReporteGasto">
            <!-- Header -->
            <ons-toolbar>
                <div class="left">
                    <strong>
                        <ons-toolbar-button onclick="consultarGastos(1);">
                            <ons-icon icon="md-long-arrow-left" 
                                    style="color: #FE8416;"></ons-icon>
                        </ons-toolbar-button>
                    </strong>
                </div>
                <div class="center" id="titleApp" style="text-align: center;"></div>
                <div class="right">
                    <ons-toolbar-button onclick="fn.open()">
                        <ons-icon icon="ion-navicon, material: md-menu"></ons-icon>
                    </ons-toolbar-button>
                </div>
            </ons-toolbar><!-- Header -->

            <!-- Titulo de pantalla -->
            <ons-card style="background-color: #0081BD;">
                <div class="tituloKronos">
                    <h3 class="center">
                        &nbsp;Reporte de Gastos
                    </h3>
                </div>
            </ons-card><!-- /titulo de pantalla -->
            
            <!-- Contenido -->
            <ons-card class="table-card">
                <form id="nuevoRepoGasto">
                    <!-- Siniestro del gasto -->
                    <ons-input type="hidden" id="gastos_repor_caso"
                                name="gastos_repor_caso"></ons-input>
                    <!-- Iniciales del usuario ajustador -->
                    <ons-input type="hidden" id="user_iniciales"
                                name="user_iniciales"></ons-input>

                    <table class="table tableKronos table-striped shadow-sm m-0">
                        <tr id="siniestroExiste"></tr>
                        <tr>
                            <td><strong>Ajustador</strong></td>
                            <td><ons-input id="user_nombre" name="user_nombre" 
                                            type="text" required></ons-input></td>
                        </tr>
                        <tr>
                            <td><strong>Moneda</strong></td>
                            <td id="gastos_moneda"></td>
                        </tr>
                        <tr>
                            <td><strong>Anticipo</strong></td>
                            <td><ons-input id="gastos_repor_anticipo" 
                                            name="gastos_repor_anticipo"
                                            placeholder="Monto $"
                                            type="number" required></ons-input></td>
                        </tr>
                    </table>
                </form>
                <div class="table-card-item">
                    <ons-button id="btnCrearReporteGasto" modifier="quiet" onclick="submitRepoGasto(event);" style="font-size:16px;width:200px;background-color:#D8DADB;color:#0081BD;">
                        <ons-icon icon="plus-square" style="color: #FE8416;"></ons-icon>
                        Crear Reporte
                    </ons-button>
                </div>
            </ons-card><!-- Contenido -->
        </ons-page>
    </ons-template><!-- Nuevo Reporte Gasto -->

    <!--Reporte de gastos-->
    <ons-template id="reporteGasto.html">
        <ons-page id="reporteGasto">
            <!-- Header -->
            <ons-toolbar>
                <div class="left">
                    <strong>
                        <ons-toolbar-button onclick="consultarGastos(1);">
                            <ons-icon icon="md-long-arrow-left" 
                                    style="color: #FE8416;"></ons-icon>
                        </ons-toolbar-button>
                    </strong>
                </div>
                <div class="right">
                    <ons-toolbar-button onclick="fn.open()">
                        <ons-icon icon="ion-navicon, material: md-menu"></ons-icon>
                    </ons-toolbar-button>
                </div>
                <div class="center" id="titleApp" style="text-align:center;"></div>
            </ons-toolbar>
            <!-- /Header -->

            <!-- Titulo de pantalla -->
            <ons-card style="background-color:#0081BD;">
                <div class="tituloKronos">
                    <ons-row>
                        <ons-col id="titlePage" style="font-size:22px;width:80%;"></ons-col>
                    </ons-row>
                </div>
            </ons-card><!-- /titulo de pantalla -->
            <!-- Contenido -->
            <ons-card class="table-card">
                <!-- Id del detalle gasto -->
                <ons-input type="hidden" id="gasto_id"></ons-input>
                <!-- Id local del gasto -->
                <ons-input type="hidden" id="idGasto"></ons-input>
                <!-- Iniciales del usuario ajustador -->
                <ons-input type="hidden" id="user_iniciales"
                            name="user_iniciales"></ons-input>
                <table class="table table-striped m-0">    
                    <tr>
                        <td><strong>Moneda</strong></td>
                        <td id="gastos_repor_moneda"></td>
                    </tr>
                    <tr>
                        <td><strong>Anticipo</strong></td>
                        <td>
                            <ons-input style="width: 160px;" type="number"
                                id="inpAnticipo" name="inpAnticipo"
                                modifier="underbar" placeholder="Anticipo" class="required">
                            </ons-input>
                        </td>
                    </tr>
                </table>
                <ons-list class="center" id="optionButtons"></ons-list>
            </ons-card>

            <ons-card class="table-card">
                <div class="table-card-item">
                    <ons-button id="btnNuevoDetalleGasto" disabled modifier="quiet" class="center" style="width: 250px;color: #0081BD;font-size: 18px;" 
                            onclick="initNuevoDetalle();">
                        <ons-icon icon="md-edit" style="width: 25px; color: #FE8416;"></ons-icon>
                        Nuevo Gasto
                    </ons-button>
                </div>
                <div id="detalleReporteContent"></div>
            </ons-card>
        </ons-page>
    </ons-template><!--Reporte de gastos-->

    <!-- Detalle de Gastos -->
    <ons-template id="detalleReporteGasto.html">
        <ons-page id="detalleReporteGasto">
            <!-- Header -->
            <ons-toolbar>
                <div class="left">
                    <strong>
                        <ons-toolbar-button onclick="backReporteGasto();">
                            <ons-icon icon="md-long-arrow-left" 
                                    style="color: #FE8416;"></ons-icon>
                        </ons-toolbar-button>
                    </strong>
                </div>
                <div class="right">
                    <ons-toolbar-button onclick="fn.open()">
                        <ons-icon icon="ion-navicon, material: md-menu"></ons-icon>
                    </ons-toolbar-button>
                </div>
                <div class="center" id="titleApp" style="text-align: center;"></div>
            </ons-toolbar><!-- Header -->
            <!-- Titulo de pantalla -->
            <ons-card style="background-color:#0081BD;">
                <div class="tituloKronos">
                    <ons-row>
                        <ons-col id="titlePage" style="font-size:22px;width:80%;"></ons-col>
                    </ons-row>
                </div>
            </ons-card><!-- /titulo de pantalla -->
            <!-- Contenido -->
            <ons-card class="table-card">
                <form id="nuevoDetalleGasto">
                    <div style="position: absolute;">
                        <ons-input type="hidden" id="gastos_id"></ons-input>
                        <ons-input type="hidden" id="idGasto"></ons-input> 
                        <ons-input type="hidden" id="idDetalleGasto"
                            name="idDetalleGasto"></ons-input>
                        <!-- Iniciales del usuario ajustador -->
                        <ons-input type="hidden" id="user_iniciales"
                            name="user_iniciales"></ons-input>
                    </div>
                    <table id="resultadoDatos" class="table table-striped shadow-sm tabla-detalle-gasto m-0">         
                            <thead id="informacion-extra">

                            </thead>
                            <tbody>
                            <tr>
                                <td><strong>Ajustador</strong></td>
                                <td><ons-input id="user_nombre" name="user_nombre" 
                                        required></ons-input></td>
                            </tr>

                            <tr>
                                <td><strong>Fecha de consumo</strong></td>
                                <td><ons-input type="date" id="gastos_fecha_real" 
                                        name="gastos_fecha_real" required></ons-input></td>
                            </tr>

                            <tr>
                                <td><strong>Concepto</strong></td>
                                <td id="gastos_concepto"></td>
                            </tr>

                            <tr>
                                <td><strong>Descripción</strong></td>
                                <td><ons-input id="gastos_descripcion" 
                                        name="gastos_descripcion" required placeholder="Descripción"></ons-input></td>
                            </tr>

                            <tr>
                                <td><strong>Razón Social</strong></td>
                                <td><ons-input id="gastos_razon_social" name="gastos_razon_social" required placeholder="Razón Social"></ons-input></td>
                            </tr>

                            <tr>
                                <td><strong>N° de Comprobante</strong></td>
                                <td><ons-input id="gastos_emisor" name="gastos_emisor" required placeholder="N° de Comprobante"></ons-input></td>
                            </tr>

                            <tr>
                                <td><strong>Total c/imp</strong></td>
                                <td><ons-input type="number" id="gastos_valor" name="gastos_valor" required placeholder="Total c/imp"></ons-input></td>
                            </tr>
