#index-page .page__background {
    background-image: url('../img/fondo_1050.jpg') !important;
    background-repeat: no-repeat;
    width:100%;
    height: 100%;
    -webkit-background-size:cover;
    -moz-background-size:cover;
    -o-background-size:cover;
    background-size:cover;
    background-position:center;
}

.btnAccess{
    background-color:rgb(10, 80, 161);
    color:white;
    border-color: white;
    border: 1px solid;
    border-radius: 8px;
    align-items: center;
    text-align: center;
    vertical-align: center;
    width: 60%;
    height: 50px;
    margin-left: 20%;
    margin-right: 20%;
    margin-top: 45%;
    padding:5px;
    font-size: 20px;
}

.btnLogin{
    background-color: white;
    color:rgb(241, 138, 41);
    border-color: rgb(241,138,41)
}